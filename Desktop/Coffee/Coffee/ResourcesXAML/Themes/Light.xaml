<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Name="themeLight">
    <SolidColorBrush x:Key="MainColorCF" Color="#704232" />
    <SolidColorBrush x:Key="HoverColorCF" Color="#B8A199" />
    <SolidColorBrush x:Key="BorderBrushCF" Color="#ECEDEE" />
    <SolidColorBrush x:Key="BackgroundCF" Color="White" />
    <SolidColorBrush x:Key="Background2CF" Color="#F7F7F7" />
    <SolidColorBrush x:Key="Background3CF" Color="#E6E6E9" />
    <SolidColorBrush x:Key="ForegroundCF" Color="Black" />
    <SolidColorBrush x:Key="RedCF" Color="Red" />
    <SolidColorBrush x:Key="GreenCF" Color="Green" />
</ResourceDictionary>