<Page x:Class="Coffee.Views.Admin.StatisticPage.StatisticPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Coffee.ViewModel.AdminVM.Statistic"
      xmlns:lvc="clr-namespace:ChartKit.SkiaSharpView.WPF;assembly=ChartKit.SkiaSharpView.WPF"
      mc:Ignorable="d" 
      xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StatisticPage">
    <Page.DataContext>
        <local:StatisticViewModel/>
    </Page.DataContext>
    <lvc:CartesianChart
        Name="lineChart"
        XAxes="{Binding XAxes}"
        YAxes="{Binding YAxes}">
    </lvc:CartesianChart>
</Page>
